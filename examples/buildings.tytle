TO DRAW_SQUARE()
  REPEAT 4 [
    FORWARD 50
    RIGHT 90
  ]
END

TO DRAW_BUILDING(H: INT)
  REPEAT H [
    DRAW_SQUARE()
    FORWARD 50
  ]
END

TO DRAW_NEIGHHBORHOOD(S: INT)
  MAKELOCAL I = 1
  MAKELOCAL X0 = 250

  REPEAT S [
    SETY 800
    MAKE X0 = X0 + 50
    SETX X0
    DRAW_BUILDING(I)
    MAKE I = I + 1
  ]
END

DRAW_NEIGHHBORHOOD(5)
